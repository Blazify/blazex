{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Compile",
      "cargo": {
        "args": [
          "build",
          "--release",
          "--features",
          "llvm"
        ],
        "filter": {
          "name": "blazescript",
          "kind": "bin"
        }
      },
      "args": [
        "examples/main.bzs",
        "-l"
      ],
      "cwd": "${workspaceFolder}"
    },
  ]
}