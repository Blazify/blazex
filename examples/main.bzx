extern int println(string, ...)
extern string str_concat(string, string)

class String {
    var value = ""

    fun(value) {
        soul.value = value
    }

    fun push_str(str) {
        soul.value = str_concat(soul.value, str)
    }
}

var str = new String("Hello")
str.push_str(" World")

println(str.value)